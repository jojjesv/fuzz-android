<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnimatedBackgroundCircle">
        <attr name="translation_x_to" format="dimension" />
        <attr name="translation_y_to" format="dimension" />
        <attr name="translation_duration" format="integer" />
    </declare-styleable>
    <declare-styleable name="ArticlesView">
        <attr name="articlesLayout" />
    </declare-styleable>

    <attr name="articlesLayout" format="enum">
        <enum name="horizontal" value="0" />
        <enum name="grid" value="1" />
        <enum name="none" value="2" />
    </attr>
</resources>